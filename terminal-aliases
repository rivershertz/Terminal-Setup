alias nr='npm run'
alias ni='npm install'
alias nui='npm uninstall'
alias gm='git merge'
alias gpl='git pull'
alias gps='git push'
alias gs='git switch'
alias gs-new='git switch -C'

**** NVM setup on Mac ****
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"


